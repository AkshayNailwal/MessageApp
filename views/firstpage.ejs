<!doctype html>
<html>
<head>
	    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
	    
</head>
<body>

    <div class="nav bg-white shadow p-1 ">
      <nav class="nav nav-underline p-2 w-100">
        <a class="nav-link active link w-50 contacts border-bottom border-primary" style='text-align:center' data-type="contacts" onclick="showList(event)" href="#">Contacts</a>
        <a class="nav-link link w-50 messages text-muted" style='text-align:center' data-type="messages" onclick="showList(event)" href="#">Messages</a>
      </nav>
    </div>
    <div class="container">
      <div class="row">
        <div class="js-lists col-lg" id="contactsList" ></div>
      </div>
      <div class="row">
        <div class="js-lists col-lg" id="messagesList" style="display:none;"></div>
      </div>
    </div>
        
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>
  var contacts = [];
  <% for(var i=0; i<data.length; i++) {%>
    contacts.push({"fname":"<%= data[i].fname %>", "lname": "<%= data[i].lname%>", "phoneno": "<%= data[i].phoneno%>"})
  <% } %>

</script>
<script src="/js/infosource.js"></script>
</body>
</html>
